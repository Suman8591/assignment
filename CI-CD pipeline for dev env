pipeline {
  agent any
  environment {
    AWS_DEFAULT_REGION = 'ap-south-1'
  }
  stages {
    stage('Checkout') {
      steps {
        git branch: 'dev1', url: 'https://github.com/Suman8591/assignment.git'
      }
    }
    stage('Build') {
      steps {
        sh "mvn package"
        }
      } 
    stage('Deploy to Apache') {
      steps {
        deploy adapters: [tomcat9(url: 'ipadress:8080',
                credentialsId: 'tomacat9cred'],
               war : 'target/.war',
               contextPath: 'app'
        }
      }
    }
